# Database Configuration
# PostgreSQL database password for local development
DB_POSTGRES_PASSWORD=your_secure_password_here

# PostgreSQL database port (default: 5432)
DB_POSTGRES_PORT=5432

# PostgreSQL database host (default: localhost for local development)
DB_POSTGRES_HOST=localhost

# API Configuration
# Port where the API server will run (default: 3000)
API_PORT=3000

# CORS Configuration
# Frontend URL for CORS (comma-separated for multiple origins)
CORS_ORIGINS=http://localhost:4200

